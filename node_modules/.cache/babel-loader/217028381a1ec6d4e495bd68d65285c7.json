port { defineComponent } from 'vue'\nimport useDOMCreate from '../hooks/useDOMCreate'\nexport default defineComponent({\n  name: 'modal',\n  props: {\n    title: String,\n    visible: {\n      type: Boolean,\n      default: false\n    }\n  },\n  emits: ['modal-on-close', 'modal-on-confirm'],\n  setup(props, context) {\n    useDOMCreate('modal')\n    const onClose = () => {\n      context.emit('modal-on-close')\n    }\n    const onConfirm = () => {\n      context.emit('modal-on-confirm')\n    }\n    return {\n      onClose,\n      onConfirm\n    }\n  }\n})\n</script>\n"],"sourceRoot":""}]}